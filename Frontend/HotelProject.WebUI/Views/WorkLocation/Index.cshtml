@model List<ResultWorkLocationDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}
<table class="table table-border">
    <tr>
        <td>#</td>
        <td>Lokasiya adı</td>
        <td>Şəhər</td>
        <td>Sil</td>
        <td>Yenilə</td>
    </tr>
    @foreach(var item in Model)
            {
				<tr>
					<td>@item.WorkLocationID</td>
					<td>@item.WorkLocationName</td>
					<td>@item.WorkLocationCity</td>
					<td>
						<a href="/WorkLocation/DeleteWorkLocation/@item.WorkLocationID" class="btn mb-1 btn-rounded btn-outline-danger">Sil</a>

					</td>
					<td>
						<a href="/WorkLocation/UpdateWorkLocation/@item.WorkLocationID" class="btn mb-1 btn-rounded btn-outline-success">Yenilə</a>

					</td>
				</tr>
            }
</table>

