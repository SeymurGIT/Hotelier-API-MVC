@model List<HotelProject.WebUI.Dtos.AppUserDto.ResultAppUserWorkLocationDto>
@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}
<div class="row">
	<div class="col-lg-12">
		<div class="card">
			<div class="card-body">
				<div class="active-member">
					<div class="table-responsive">
						<table class="table table-xs mb-0">
							<thead>
								<tr>
									<th>İstifadəçilər</th>
									<th>Telefon nömrəsi</th>
									<th>Yaşadığı şəhər</th>
									<th>Status</th>
									<th>Ən son aktivlik</th>
									<th>Ən son aktivlik</th>
								</tr>
							</thead>
							<tbody>
								@foreach (var item in Model)
								{
								<tr>
									<td><img src="@(item.ImageUrl ?? "Boş")" class=" rounded-circle mr-3" alt="">@item.Name @item.Surname </td>
									@if (item.PhoneNumber is null)
									{
										<td>
											Boş
										</td>
									}
									else
									{
										@($"{item.PhoneNumber.Substring(0,3)}-{item.PhoneNumber.Substring(3, 3)}-{item.PhoneNumber.Substring(6, 2)}-{item.PhoneNumber.Substring(8, 2)}")
									}
									<td>
										<span>@(item.City ?? "Boş")</span>
									</td>
									<td>
										<div>
											<div class="progress" style="height: 6px">
												<div class="progress-bar bg-success" style="width: 50%"></div>
											</div>
										</div>
									</td>
									<td><i class="fa fa-circle-o text-success  mr-2"></i> Paid</td>
									<td>
										<span>Last Login</span>
										<span class="m-0 pl-3">10 sec ago</span>
									</td>

									
								</tr>
								}
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

