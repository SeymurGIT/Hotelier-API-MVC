@model UpdateAboutDto 
@{
    ViewData["Title"] = "UpdateAbout";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}
<form method="post">
	<input type="hidden" asp-for="AboutId" />
	<label>Başlıq 1 (Qara)</label>
	<input type="text" asp-for="Title1" class="form-control" value="@(Model.Title1 == "string" ? "" : Model.Title1)" />
	<br />
	<label>Başlıq 2 (Narıncı)</label>
	<input type="text" asp-for="Title2" class="form-control" value="@(Model.Title2 == "string" ? "" : Model.Title2)"/>
	<br />
	<label>Otaq sayı</label>
	<input type="text" asp-for="RoomCount" class="form-control" />
	<br />
	<label>Müştəri sayı</label>
	<input type="text" asp-for="CustomerCount" class="form-control" />
	<br />
	<label>İşçi sayı</label>
	<input type="text" asp-for="StaffCount" class="form-control" />
	<br />
	<button class="btn mb-1 btn-rounded btn-outline-success">Məlumatları yenilə</button>


</form>

